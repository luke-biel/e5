{
  "name": "e5",
  "version": "0.1.0",
  "exports": "./src/main.ts",
  "tasks": {
    "run": "deno run --allow-read --allow-write --allow-run --allow-env --allow-net src/main.ts",
    "install": "deno install --allow-read --allow-write --allow-run --allow-env --allow-net -n e5 src/main.ts",
    "compile": "deno compile --allow-read --allow-write --allow-run --allow-env --allow-net -o e5 src/main.ts",
    "serve-repo": "deno run --allow-net --allow-read scripts/serve-repo.ts"
  },
  "imports": {
    "@std/toml": "jsr:@std/toml@^1",
    "@std/cli": "jsr:@std/cli@^1",
    "@std/fmt": "jsr:@std/fmt@^1",
    "@std/path": "jsr:@std/path@^1",
    "@std/fs": "jsr:@std/fs@^1"
  }
}
